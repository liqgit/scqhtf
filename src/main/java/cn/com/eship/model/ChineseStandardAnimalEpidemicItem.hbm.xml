<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.com.eship.model.ChineseStandardAnimalEpidemicItem" table="t_chinese_standard_animal_epidemic_item"
           schema="tjrsms">
        <id name="id" column="id"/>
        <property name="diseaseNameEng" column="disease_name_eng"/>
        <property name="diseaseNameCn" column="disease_name_cn"/>
        <property name="diseaseClass" column="disease_class"/>
    </class>
</hibernate-mapping>